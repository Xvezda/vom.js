{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "useBuiltIns": "entry",
        "corejs": {"version": 3, "proposals": true},
        "exclude": [
          "@babel/plugin-transform-typeof-symbol"
        ]
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}
